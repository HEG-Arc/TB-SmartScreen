<Window x:Class="POC_UserAwareness.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POC_UserAwareness"
        mc:Ignorable="d"
        Title="POC - User Awareness" Width="960" Height="540"
        ResizeMode="NoResize">
    <StackPanel>
        <Grid Width="960" Height="540">
            <Image Name="image" VerticalAlignment="Top" />
            <Canvas Name="bodyCanvas" VerticalAlignment="Top" />
        </Grid>
    </StackPanel>
</Window>
